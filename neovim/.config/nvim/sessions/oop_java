let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/vault/2:1::Semester/ZNotes
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +1 OOPS/javaEnv/cases/t0.java
badd +1 ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/log.txt
badd +1 ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/t1.java
badd +1 ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/t2.java
badd +1 ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/t3.java
badd +0 ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/ainput.txt
badd +0 ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/aoutput.txt
argglobal
%argdel
$argadd .
edit OOPS/javaEnv/cases/t0.java
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
wincmd _ | wincmd |
split
1wincmd k
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 129 + 96) / 192)
exe '2resize ' . ((&lines * 19 + 21) / 42)
exe 'vert 2resize ' . ((&columns * 62 + 96) / 192)
exe '3resize ' . ((&lines * 19 + 21) / 42)
exe 'vert 3resize ' . ((&columns * 62 + 96) / 192)
argglobal
balt ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/log.txt
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 1 - ((0 * winheight(0) + 19) / 39)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 1
normal! 0
wincmd w
argglobal
if bufexists(fnamemodify("~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/ainput.txt", ":p")) | buffer ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/ainput.txt | else | edit ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/ainput.txt | endif
if &buftype ==# 'terminal'
  silent file ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/ainput.txt
endif
balt ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/aoutput.txt
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 1 - ((0 * winheight(0) + 9) / 19)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 1
normal! 0
wincmd w
argglobal
if bufexists(fnamemodify("~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/aoutput.txt", ":p")) | buffer ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/aoutput.txt | else | edit ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/aoutput.txt | endif
if &buftype ==# 'terminal'
  silent file ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/aoutput.txt
endif
balt ~/vault/2:1::Semester/ZNotes/OOPS/javaEnv/cases/ainput.txt
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let &fdl = &fdl
let s:l = 3 - ((2 * winheight(0) + 9) / 19)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 3
normal! 0
wincmd w
exe 'vert 1resize ' . ((&columns * 129 + 96) / 192)
exe '2resize ' . ((&lines * 19 + 21) / 42)
exe 'vert 2resize ' . ((&columns * 62 + 96) / 192)
exe '3resize ' . ((&lines * 19 + 21) / 42)
exe 'vert 3resize ' . ((&columns * 62 + 96) / 192)
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
nohlsearch
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
